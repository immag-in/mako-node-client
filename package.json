{
  "name": "@immagin/client",
  "version": "1.0.1",
  "description": "Immagin nodeJS client",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/module/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "rimraf lib && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "watch:build": "tsc -p tsconfig.json -w",
    "format": "prettier --write .",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immag-in/mako-node-client.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "immagin",
    "nodejs",
    "client",
    "mako",
    "image",
    "processing"
  ],
  "author": "Immagin <hello@immag.in> (https://immag.in)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/immag-in/mako-node-client/issues"
  },
  "homepage": "https://github.com/immag-in/mako-node-client#readme",
  "devDependencies": {
    "@types/exif": "^0.6.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "jest": "^27.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@exivar/funtry": "^1.0.2",
    "axios": "^0.23.0",
    "chalk": "^4.1.2",
    "exif": "^0.6.0"
  }
}
