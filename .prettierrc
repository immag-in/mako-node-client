{
  "bracketSpacing": true,
  "printWidth": 120,
  "proseWrap": "never",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "useTabs": true,
  "overrides": [
    {
      "files": [".prettierrc", "*.json"],
      "options": {
        "printWidth": 200,
        "tabWidth": 2,
        "useTabs": false
      }
    }
  ]
}
